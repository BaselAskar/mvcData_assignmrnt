@model List<Person>

@if (Model.Count == 0)
{
    <div class="no-people">
        No Peson found ....!
    </div>
}
else
{
    <div class="row py-3">
        <div class="col-3 my-auto fw-bold">Name</div>
        <div class="col-6 my-auto fw-bold">City</div>

    </div>
}


@foreach (Person person in Model)
{
    <partial name="_PersonRow" model="person" />
}


